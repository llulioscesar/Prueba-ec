{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "comment-format": [
      true,
      "check-space"
    ],
    "member-access": false,
    "ordered-imports": false,
    "max-line-length": [
      true,
      450
    ],
    "interface-name": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "indent": [
      true,
      "tabs",
      4
    ],
    "no-trailing-whitespace": false,
    "noUnusedLocals": true,
    "noUnusedParameters":true,
    "no-empty": false,
    "forin": true
  },
  "rulesDirectory": [],
}